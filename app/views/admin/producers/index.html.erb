<div class="left-column">
<table>
  <tr>
    <th>Producer Name</th>
    <th>Edit</th>
    <th>Manage Products</th>
    <th>Delete</th>
  </tr>
<% @accounts.each do |account| %>
  <tr>
    <td><%= link_to account.producer_name, user_path(account.creator) %></td>
    <td><%= link_to 'Edit', edit_account_path(account) %></td>
    <td><%= link_to 'Manage Products', account_path(account) %></td>
    <td><%= link_to 'Delete', account_path(account), :method => :delete %></td>
  </tr>
<% end %>
</table>
</div>