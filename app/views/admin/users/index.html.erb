<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Producer</th>
    <th>Admin</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%=h user.account.first_name %></td>
    <td><%=h user.account.last_name %></td>
    <td><%=h user.email %></td>
    <td><%= (user.account.producer ? 'yes' : 'no') %></td>
    <td><%= (user.account.admin ? 'yes' : 'no') %></td>
    <td><%= link_to 'Edit', edit_account_path(user.account) %></td>
    <td><%= link_to 'Delete', account_path(user.account), :method => :delete %>
<% end %>
</table>