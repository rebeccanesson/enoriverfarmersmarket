<h1>New order</h1>

<% form_for(@order) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :delivery_cycle_id %><br />
    <%= f.text_field :delivery_cycle_id %>
  </p>
  <p>
    <%= f.label :final %><br />
    <%= f.check_box :final %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', orders_path %>